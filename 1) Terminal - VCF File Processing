lidysce@Wall-E:/mnt/c/Users/lidys/UFL Dropbox/Lidysce Mata Cantarero/Lidysce - Heat Tolerance/Lidysce/Classes/HOS6236 - Molecular Marker Assisted PlantBreeding/GWAS Project$ awk -F'\t' -v OFS='\t' '
NR==1{
  print $1,$2,$3,$4,$5;
  for(i=6;i<=NF;i+=2) {
    sub(":RO","",$i);
    printf "%s%s", $i, (i+2<=NF?OFS:ORS);
  }
  next
}
{
  printf "%s\t%s\t%s\t%s\t%s", $1,$2,$3,$4,$5;
  for(i=6;i<=NF;i+=2){
    r=$i; a=$(i+1);
    if(r=="" || a=="" || r=="." || a=="." ){ val="NA" }
    else{
      d=r+a;
      if(d==0){ val="NA" }
      else{
        p=a/d;
        if(p<0.25) val=0;
        else if(p>0.75) val=2;
        else val=1;
      }
    }
    printf "%s%s", OFS, val;
  }
  printf "\n";
}' AD.tsv > dosage.tsv
